<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.3.xsd">
       
	<bean id="user" class="com.spring.demo4.User">
		<constructor-arg value="helloword"></constructor-arg>
		<constructor-arg value="baidu"></constructor-arg>
		<constructor-arg value="24"></constructor-arg>
		<constructor-arg value="true"></constructor-arg>
	</bean>
	
	<bean id="user2" class="com.spring.demo4.User">
		<constructor-arg name="name" value="hellowold111" />
		<constructor-arg name="age" value="11" />
	</bean>
</beans>