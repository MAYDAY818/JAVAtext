package com.hibernate.entity;

import java.util.List;

public class Customer {
	//用户实体
	
	//用户名
	private String name;
	//密码
	private String pwd;
	//收货地址
	private List<Adress> adress;
	//手机
	private String phone;
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
	public List<Adress> getAdress() {
		return adress;
	}
	public void setAdress(List<Adress> adress) {
		this.adress = adress;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public Customer(String name, String pwd, List<Adress> adress, String phone) {
		super();
		this.name = name;
		this.pwd = pwd;
		this.adress = adress;
		this.phone = phone;
	}
	public Customer() {
		super();
	}
	@Override
	public String toString() {
		return "Customer [name=" + name + ", pwd=" + pwd + ", adress=" + adress + ", phone=" + phone + "]";
	}
	
	
}
